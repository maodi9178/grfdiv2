ETab:AddToggle({
    Name = "存满了自动售卖",
    Desc = "库存满时自动售卖",
    Default = false,
    IsMobile = false,
    Flag = "AutoSellMaxToggle",
    Save = true,
    Callback = function(Value)
        autosellmax = Value
        if autosellmax then
            maxsell()
        end
    end
})

